{
  "name": "inale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "test": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:seed": "npx prisma db seed",
    "db:generate-migration": "npx prisma migrate dev --create-only --name",
    "db:migrate": "npx prisma migrate dev",
    "db:restart": "mysql --defaults-extra-file=.mysql-local.conf -e \"drop database inale; create database inale;\" && npm run db:push && npm run db:seed;",
    "db:restart-aws": "mysql --defaults-extra-file=.mysql-aws.conf -e \"drop database inale; create database inale;\" && npm run db:push && npm run db:seed;"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
